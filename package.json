{
  "name": "chessSolid",
  "version": "1.0.0",
  "description": "base chess app , later to integrate solidity as backend",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build -- --watch --mode=development",
    "seed": "node script/seed.js",
    "start": "node server",
    "start:windows": "set JWT=shh&&nodemon server",
    "start:dev": "npm run build:dev & npm run start-server",
    "start:dev:logger": "LOGGING=true npm run start:dev",
    "start:dev:seed": "SEED=true npm run start:dev",
    "start-server": "JWT=shh nodemon server -e html,js,scss --ignore public --ignore client",
    "test": "NODE_ENV=test JWT=a_secret mocha \"./server/**/*.spec.js\" \"./client/**/*.spec.js\" --require @babel/register --require esm ",
    "test:dev": "npm run test -- --watch",
    "test:dev:models": "npm run test:dev -- --grep=model",
    "test:dev:routes": "npm run test:dev -- --grep=route"
  },
  "author": "Anatoly Tsinker",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "chess.js": "^0.12.0",
    "chessboardjsx": "^2.4.7",
    "compression": "^1.7.3",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "history": "^4.9.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "nodemon": "^1.19.4",
    "pg": "^8.5.1",
    "sequelize": "^6.3.5",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "axios": "^0.21.1",
    "axios-mock-adapter": "^1.16.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.2.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "esm": "^3.2.25",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scroll": "^1.8.4",
    "react-scroll-to-bottom": "^4.2.0",
    "react-test-renderer": "^16.4.2",
    "supertest": "^4.0.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  }
}
